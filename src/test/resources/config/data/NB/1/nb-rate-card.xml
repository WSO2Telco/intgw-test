<RateCard>
	<Operators>
		<API name="SMS">
			<!-- CONSTANTdefault -->
			<Operation name="200">
				<Rate default="true">
					<Name>SM1</Name>
					<Currency>Rs</Currency>
					<Value>10</Value>
					<Type>CONSTANT</Type>
					<Taxes>
						<Tax>VAT</Tax>
						<Tax>ABC</Tax>
					</Taxes>
				</Rate>
			</Operation>
			<!-- CONSTANTnon-default categoryANDsubcategory -->
			<Operation name="201">
				<Rate>
					<Name>SM2</Name>
					<Currency>Rs</Currency>
					<Value>10</Value>
					<Type>CONSTANT</Type>
					<CategoryBase>true</CategoryBase>
					<Category name="349" rate="12">
						<SubCategory name="651" rate="100" />
						<SubCategory name="652" rate="200" />
						<SubCategory name="653" rate="300" />
					</Category>
					<Taxes>
						<Tax>VAT</Tax>
						<Tax>ABC</Tax>
					</Taxes>
				</Rate>
			</Operation>
			<!-- PER_REQUEST default -->
			<Operation name="202">
				<Rate default="true">
					<Name>SM3</Name>
					<Currency>Rs</Currency>
					<Value>10</Value>
					<Type>PER_REQUEST</Type>
					<Taxes>
						<Tax>VAT</Tax>
						<Tax>ABC</Tax>
					</Taxes>
				</Rate>
			</Operation>
		</API>
		<API name="USSD">
			<!-- QUOTA default -->
			<Operation name="400">
				<Rate default="true">
					<Name>ul</Name>
					<Currency>Rs</Currency>
					<Value>20</Value>
					<Type>QUOTA</Type>
					<Attributes>
						<MaxCount>500</MaxCount>
						<ExcessRate>10</ExcessRate>
						<DefaultRate>10</DefaultRate>
					</Attributes>
					<Taxes>
						<Tax>VAT</Tax>
						<Tax>ABC</Tax>
					</Taxes>
				</Rate>
			</Operation>
		</API>
		<API name="location">
			<!-- QUOTA non-default categoryANDsubcategory -->
			<Operation name="300">
				<Rate>
					<Name>lbs</Name>
					<Currency>Rs</Currency>
					<Value>20</Value>
					<Type>QUOTA</Type>
					<CategoryBase>true</CategoryBase>
					<Attributes>
						<MaxCount>500</MaxCount>
						<ExcessRate>10</ExcessRate>
						<DefaultRate>10</DefaultRate>
					</Attributes>
					<Category name="Game">
						<Attributes>
							<MaxCount>500</MaxCount>
							<ExcessRate>10</ExcessRate>
							<DefaultRate>10</DefaultRate>
						</Attributes>
						<SubCategory name="Game01">
							<Attributes>
								<MaxCount>500</MaxCount>
								<ExcessRate>10</ExcessRate>
								<DefaultRate>10</DefaultRate>
							</Attributes>
						</SubCategory>
						<SubCategory name="Game02">
							<Attributes>
								<MaxCount>500</MaxCount>
								<ExcessRate>10</ExcessRate>
								<DefaultRate>10</DefaultRate>
							</Attributes>
						</SubCategory>
						<SubCategory name="Game03">
							<Attributes>
								<MaxCount>500</MaxCount>
								<ExcessRate>10</ExcessRate>
								<DefaultRate>10</DefaultRate>
							</Attributes>
						</SubCategory>
					</Category>
				</Rate>
			</Operation>
		</API>
		<API name="payment">
			<!-- PERCENTAGE categoryBaseFalse -->
			<Operation name="100">
				<Rate default="true">
					<Name>p1</Name>
					<Currency>Rs</Currency>
					<Value>20</Value>
					<Commission>
						<Sp>80</Sp>
						<Ads>15</Ads>
						<Opco>5</Opco>
					</Commission>
					<Type>PERCENTAGE</Type>
				</Rate>
			</Operation>
			<!-- PER_REQUEST withSurCharge non-default categoryANDsubcategory -->
			<Operation name="101">
				<Rate>
					<Name>RF2</Name>
					<Currency>Rs</Currency>
					<Value>120</Value>
					<Surcharge value="100" Ads="90" Opco="10" />
					<Type>PER_REQUEST</Type>
					<Category name="Game" rate="10">
						<SubCategory name="Game01" rate="100" />
						<SubCategory name="Game02" rate="200" />
						<SubCategory name="Game03" rate="300" />
					</Category>
				</Rate>
			</Operation>
		</API>
	</Operators>
</RateCard>
	